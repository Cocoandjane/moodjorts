{
    "version": 2,
    "builds": [
        {
          "src": "app.js",
          "use": "@vercel/node",
          "output": "dist/server.js",
          "env": {
            "NODE_ENV": "production"
          }
        }
      ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}